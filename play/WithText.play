<script>
  import Vuex from 'vuex'
  import MyButton from './MyButton.vue'

  export default {
    //name: 'with-text',
    scenario: 'with text', // scenario
    template: `<my-button :handle-click="log">{{ $store.state.count }}</my-button>`,
    components: { MyButton },
    store: new Vuex.Store({
      state: { count: 0 }
    }),
    methods: {
      log () {
        this.$store.state.count++
      }
    }
  }
</script>

